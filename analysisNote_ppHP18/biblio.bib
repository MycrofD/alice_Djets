	Eprint = {1505.00664},
	Issn = {1029-8479},
	Journal = {Journal of High Energy Physics},
	Keywords = {LHC, ALICE, heavy quarks, charm, bottom},
	Number = {9},
	Pages = {1--46},
	Primaryclass = {nucl-ex},
	Title = {{Measurement of charm and beauty production at central rapidity versus charged-particle multiplicity in proton-proton collisions at $\sqrt{s}$ = 7 TeV}},
	Volume = {2015},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/JHEP09(2015)148}}

